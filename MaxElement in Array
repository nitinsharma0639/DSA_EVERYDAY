
public class MaxArray {
	
	public static int maxNum(int[] arr) {
		int max=arr[0];
		for(int i = 0; i<arr.length;i++) {
			if(max < arr[i]) {
				max = arr[i];
			}
		}
		return max;
	}

	public static void main(String[] args) {
		int[] arr= {2,5,17,8,13,0};
		
		int maxNum = maxNum(arr);
		System.out.println(maxNum);

	}

}
