
public class MinArray {

	public static int minNum(int[] arr) {
		
		int min=arr[0];
		for(int i = 0; i<arr.length;i++) {
			if(min > arr[i]) {
				min= arr[i];
				
			}
		}
		return min;
	}
	
	public static void main(String[] args) {

		int[] arr = {20, 4,13, 17, 99, 2, 80};
		System.out.println(minNum(arr));
	}

}
